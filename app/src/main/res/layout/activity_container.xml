        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/bottom_image"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@drawable/bottm_nav"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_news_feed"
            android:layout_width="@dimen/margin_24"
            android:layout_height="@dimen/margin_24"
            android:src="@drawable/ic_news_feed"
            app:layout_constraintLeft_toRightOf="@+id/image_view_buy_coin"
            android:layout_marginStart="@dimen/margin_24"
            android:layout_marginBottom="@dimen/margin_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_buy_coin"
            android:layout_width="@dimen/margin_24"
            android:layout_height="@dimen/margin_24"
            android:layout_marginLeft="100dp"
            android:src="@drawable/ic_buycoin"
            app:layout_constraintRight_toLeftOf="@+id/image_view_news_feed"
            android:layout_marginBottom="@dimen/margin_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_categories"
            android:layout_width="@dimen/margin_24"
            android:layout_height="@dimen/margin_24"
            android:layout_marginBottom="@dimen/margin_24"
            android:layout_marginEnd="110dp"
            android:src="@drawable/ic_group"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/image_view_activity"
            android:layout_width="@dimen/margin_24"
            android:layout_height="@dimen/margin_24"
            android:src="@drawable/ic_activity"
            android:layout_marginEnd="@dimen/margin_24"
            android:layout_marginBottom="@dimen/margin_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>


        <View
            android:layout_width="124dp"
            android:layout_height="3dp"
            android:background="#303030"
            android:layout_marginBottom="4dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>



    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>


